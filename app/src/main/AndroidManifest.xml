<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.study.gst.kisa_hack">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".myEmail"
        android:allowBackup="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="돌려듀오"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DeptorSignatureActivity"></activity>
        <activity android:name=".FirstReceiveActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".SignatureActivity" />
        <activity android:name=".FirstSendActivity" />
        <activity android:name=".chat.MessageActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppFullScreenTheme"
            android:icon="@drawable/back_logo">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>